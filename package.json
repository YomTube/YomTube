{
	"name": "yomtube",
	"description": "clean",
	"version": "0.0.1",
	"scripts": {
		"dev": "TYPE=local env-cmd -f ./.env sapper dev",
		"dev:public": "TYPE=public env-cmd -f ./.env sapper dev",
		"setup": "npm install && mongorestore DBExport/ && node -e 'console.log(\"All done!\")'",
		"clean-database": "mongo yomtube --eval 'db.dropDatabase()' && mongorestore DBExport/",
		"build": "sapper build --legacy",
		"export": "sapper export --legacy",
		"start": "node __sapper__/build",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"test": "run-p --race dev cy:run"
	},
	"dependencies": {
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"compression": "^1.7.1",
		"env-cmd": "^10.0.1",
		"express": "^4.17.1",
		"express-fileupload": "^1.1.6",
		"fluent-ffmpeg": "^2.1.2",
		"jimp": "^0.9.5",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.8.9",
		"multer": "^1.4.2",
		"node-sass": "^4.13.1",
		"polka": "next",
		"scss": "^0.2.4",
		"sirv": "^0.4.0",
		"svelte-preprocess": "^3.4.0",
		"validator": "^12.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/plugin-syntax-dynamic-import": "^7.0.0",
		"@babel/plugin-transform-runtime": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"@babel/runtime": "^7.0.0",
		"@rollup/plugin-commonjs": "^11.0.0",
		"@rollup/plugin-json": "^4.0.1",
		"@rollup/plugin-node-resolve": "^6.0.0",
		"@rollup/plugin-replace": "^2.2.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.19.1",
		"rollup": "^1.20.0",
		"rollup-plugin-babel": "^4.0.2",
		"rollup-plugin-svelte": "^5.0.1",
		"rollup-plugin-terser": "^5.2.0",
		"sapper": "^0.27.11",
		"svelte": "^3.0.0"
	}
}
